<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<HBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="myApp.App">
    <children>
        <Pane fx:id="pane">
            <children>
                <Canvas fx:id="canvas" height="800.0" width="1200.0"/>
            </children>
        </Pane>
        <VBox alignment="CENTER" prefHeight="400.0" prefWidth="200.0" spacing="8.0">
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
            <children>
                <Button fx:id="pause" mnemonicParsing="false" text="start">
                    <font>
                        <Font name="Die Hund  Normal" size="12.0"/>
                    </font>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </VBox.margin>
                </Button>
                <Button fx:id="reset" mnemonicParsing="false" text="Reset" textAlignment="CENTER">
                    <font>
                        <Font name="Die Hund  Normal" size="12.0"/>
                    </font>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </VBox.margin>
                </Button>
                <Button fx:id="expand" mnemonicParsing="false" text="expand">
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                    <font>
                        <Font name="Die Hund  Normal" size="12.0"/>
                    </font>
                </Button>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="background">
                    <font>
                        <Font name="Die Hund  Normal" size="12.0"/>
                    </font>
                </Text>
                <ColorPicker fx:id="background"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="cell">
                    <font>
                        <Font name="Die Hund  Normal" size="12.0"/>
                    </font>
                </Text>
                <ColorPicker fx:id="cell"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="border">
                    <font>
                        <Font name="Die Hund  Normal" size="12.0"/>
                    </font>
                </Text>
                <ColorPicker fx:id="border"/>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="evolving speed">
                    <font>
                        <Font name="Die Hund  Normal" size="12.0"/>
                    </font>
                </Text>
                <ChoiceBox fx:id="speed" maxWidth="-Infinity" prefWidth="65.0"/>
            </children>
        </VBox>
    </children>
</HBox>
